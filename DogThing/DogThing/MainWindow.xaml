<Window x:Class="DogThing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DogThing"
        mc:Ignorable="d"
        Title="MainWindow" Height="1500" Width="2000" WindowState="Maximized">
    <Canvas Name="canv">
        <Ellipse Name="track" Panel.ZIndex="200" Visibility="Hidden" Width="10" Height="10" Fill="Black" Opacity="1" Canvas.Left="0" Canvas.Top="0"></Ellipse>
        <Rectangle Name="dogger" Panel.ZIndex="50" Width="10" Height="20" Fill="Black" RenderTransformOrigin="0.5,0.5" Canvas.Left="100" Canvas.Top="100">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="35.062"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
        <TextBlock Name="tAngle" Text=""></TextBlock>
        <TextBlock Name="dAngle" Text="" Canvas.Top="16"></TextBlock>
        <Rectangle Name="Clickeroni" Visibility="Hidden" Panel.ZIndex="100" Width="2000" Height="2000" Fill="Black" Opacity="0" PreviewMouseDown="Clickeroni_PreviewMouseDown"/>
        <Line Name="wV" Visibility="Hidden" Y1="0" Y2="2000" X1="0" X2="0" StrokeThickness="1" Stroke="Red"></Line>
        <Line Name="wH" Visibility="Hidden" X1="0" X2="2000" Y1="0" Y2="0" StrokeThickness="1" Stroke="Red"></Line>
        <Rectangle Name="x1" Width="100" Opacity="1" Height="1118" Fill="Blue" Canvas.Left="422" Canvas.Top="354"></Rectangle>
        <Rectangle Name="xa2" Width="100" Height="846" Fill="Gray" Canvas.Left="184" Canvas.Top="0"/>
        <Rectangle Name="xa3" Width="100" Height="424" Fill="Gray" Canvas.Left="422" Canvas.Top="-168"/>
        <Rectangle Name="x4" Width="643" Height="100" Fill="Blue" Canvas.Left="522" Canvas.Top="156"/>
        <Rectangle Name="xa5" Width="100" Opacity="1" Height="437" Fill="Gray" Canvas.Left="654" Canvas.Top="409"/>
        <Rectangle Name="x6" Width="100" Opacity="1" Height="156" Fill="Blue" Canvas.Left="654" Canvas.Top="253"/>
        <Rectangle Name="xa7" Width="100" Opacity="1" Height="478" Fill="Gray" Canvas.Left="873" Canvas.Top="253"/>
        <Rectangle Name="xa9" Width="192" Opacity="1" Height="100" Fill="Gray" Canvas.Left="1128" Canvas.Top="631"/>
        <Rectangle Name="x10" Width="193" Opacity="1" Height="100" Fill="Blue" Canvas.Left="972" Canvas.Top="631"/>

    </Canvas>
</Window>
